package mlyv.app;

import java.io.Serializable;

public class AtomPayment implements Serializable {
	private static final long serialVersionUID = -5435670920302756945L;
	
	private String name = "";
	private String value = "";
	private String id = "";
	private String lat = "";
	private String lon = "";
	private String idCat = "";

	public AtomPayment(String id, String name, String value, String lat, String lon, String idCat) {
		this.setName(name);
		this.setValue(value);
		this.id = id;
		this.lat = lat;
		this.lon = lon;
		this.idCat = idCat;
	}
	
	public String getIdCat() {
		return idCat;
	}

	public String getId() {
		return id;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getValue() {
		return value;
	}
	
	public String getLat() {
		return lat;
	}
	
	public String getLon() {
		return lon;
	}

	public void setValue(String value) {
		this.value = value;
	}
}